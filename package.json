{
  "name": "abhi-examples",
  "displayName": "abhi-examples",
  "publisher": "kr-abhi55",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode.extension.example.createREADME",
        "title": "Create README"
      },
      {
        "command": "vscode.extension.example.create.gitignore",
        "title": "Create .gitignore"
      },
      {
        "command": "vscode.extension.example.create.CMakeLists.txt",
        "title": "Create CMakeLists.txt"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode.extension.example.createREADME",
          "group": "abhi",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode.extension.example.create.gitignore",
          "group": "abhi",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "vscode.extension.example.create.CMakeLists.txt",
          "group": "abhi",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
